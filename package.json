{
  "name": "yunsol-land",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -n SERVER,CLIENT -c magenta,cyan \"npm:dev --workspace server\" \"npm:dev --workspace client\"",
    "build": "npm run build --workspace server && npm run build --workspace client",
    "prisma:generate": "npm run prisma:generate --workspace server",
    "prisma:migrate": "npm run prisma:migrate --workspace server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
